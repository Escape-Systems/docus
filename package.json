{
  "name": "@larbish/docus",
  "description": "Minimal Documentation theme and CLI for shared usage across Nuxt modules",
  "private": false,
  "version": "1.0.0",
  "keywords": [],
  "license": "MIT",
  "bin": {
    "docus": "dist/main.mjs"
  },
  "files": [
    "app",
    "dist",
    "!app/**/node_modules",
    "!app/**/.nuxt",
    "!app/**/tsconfig.json"
  ],
  "scripts": {
    "dev": "npm run docs:dev",
    "docs:dev": "NUXT_DOCS_DEV=1 npm run docus dev docs",
    "docs:build": "npm run docus build docs",
    "dev:prepare": "nuxi prepare app && nuxi prepare docs",
    "build": "tsup-node ./cli/main.ts --format esm",
    "docus": "tsx ./cli/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.40",
    "@iconify-json/simple-icons": "^1.2.33",
    "@nuxt/content": "^3.5.1",
    "@nuxt/image": "^1.10.0",
    "@nuxt/ui-pro": "^3.1.1",
    "c12": "^3.0.3",
    "citty": "^0.1.6",
    "dotenv": "^16.5.0",
    "git-url-parse": "^16.1.0",
    "nuxi": "^3.25.0",
    "nuxt": "^3.17.2",
    "nuxt-og-image": "^5.1.3",
    "pkg-types": "^2.1.0",
    "tailwindcss": "^4.1.5",
    "unctx": "^2.4.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.3.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.26.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
