{
  "name": "docus-workspace",
  "description": "Minimal Documentation theme and CLI for shared usage across Nuxt modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxtlabs/docus.git"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm run docs:dev",
    "docs:dev": "cd docs && nuxt dev --extends ../layer",
    "docs:build": "nuxt build docs --extends ../layer",
    "dev:prepare": "nuxt prepare layer && nuxt prepare docs --extends ../layer",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck layer && nuxi typecheck docs --extends ../layer",
    "cli:release": "release-it --config .release-it.cli.json",
    "layer:release": "release-it --config .release-it.layer.json"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.7.0",
    "@nuxt/eslint-config": "^1.5.2",
    "@release-it/conventional-changelog": "^10.0.1",
    "eslint": "^9.31.0",
    "nuxt": "^4.0.0",
    "release-it": "^19.0.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.13.1",
  "version": ""
}
